[user]
        name = psyanite
        email = psyanite@gmail.com
[color]
        diff = auto
        status = auto
        branch = auto
        interactive = auto
[alias]
        st = status
        ci = commit
        co = checkout
        sb = submodule update --init --recursive
        br = branch
        cp = cherry-pick
        mt = mergetool
        dt = difftool
        la = log --graph --decorate --oneline --abbrev-commit
        li = log --graph --decorate --oneline --abbrev-commit -10
        lo = log --format=format:'%C(bold magenta)%h%C(reset) - %C(auto)%d%C(reset) %C(white)%s%C(reset) %C(bold dim white)- %an%C(reset) %C(dim white)(%ar)%C(reset) %C(dim white)(%ad)%C(reset)' --all --date=local --graph --abbrev-commit --decorate
        rb = rebase
 
[core]
        eol = lf
[log]
        date = local
        abbrevCommit = true
[push]
        default = upstream
